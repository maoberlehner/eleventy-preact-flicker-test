{
  "name": "eleventy-preact-flicker-test",
  "version": "0.1.0",
  "author": "Markus Oberlehner",
  "homepage": "https://github.com/maoberlehner/eleventy-preact-flicker-test",
  "license": "MIT",
  "private": true,
  "scripts": {
    "setup": "chmod 0755 bin/setup && bin/setup",
    "build": "NODE_ENV=production concurrently 'eleventy' 'yarn scripts:app' 'BROWSERSLIST_ENV=modern yarn scripts:app' 'yarn styles'",
    "dev": "NODE_ENV=dev concurrently 'eleventy --serve' 'yarn watch'",
    "scripts:app": "rollup -c",
    "scripts:app:watch": "yarn scripts:app -- --watch",
    "styles": "node-sass --output-style compressed --importer node_modules/node-sass-magic-importer/dist/cli.js -o dist src/styles/index.scss",
    "styles:watch": "chokidar 'src/**/*.scss' -c 'yarn styles'",
    "watch": "concurrently 'yarn scripts:app:watch' 'yarn styles:watch'",
    "lint:scripts": "eslint --ext .js .",
    "lint:styles": "stylelint 'src/**/*.scss'",
    "lint": "concurrently 'yarn lint:scripts' 'yarn lint:styles'"
  },
  "dependencies": {
    "htm": "^3.0.3",
    "preact": "^10.3.4",
    "reset-css": "^5.0.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@avalanche/eslint-config": "^4.0.0",
    "@avalanche/stylelint-config": "^1.0.1",
    "@babel/preset-env": "^7.9.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-htm": "^3.0.0",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^5.1.0",
    "core-js": "^3.6.4",
    "eslint": "^6.8.0",
    "eslint-config-preact": "^1.1.1",
    "eslint-plugin-compat": "^2.6.3",
    "eslint-plugin-import": "^2.20.2",
    "node-sass": "^7.0.0",
    "node-sass-magic-importer": "^5.3.2",
    "preact-render-to-string": "^5.1.4",
    "rollup": "^2.3.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "stylelint": "^13.2.1"
  }
}
